<li id="<%= feed_item.id %>"> 
 
  <span class="content"><h3><%= feed_item.content %></h3></span>
  </br>
  <span>
    <h4>Friends aboard this mission:</h4> <h3><%= feed_item.followers.count %></h3>
  </span></br>  
  <span>
  <%= tweet_button(:via => "Empowler", :text => feed_item.content, :count => "horizontal", :related => "Empowler") %>	  
  </span>
	</br>
  <span><%= facebook_like %> </span>
  <span class="timestamp">
    Posted <%= time_ago_in_words(feed_item.created_at) %>
  </span>
</li>
